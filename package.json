{
  "name": "watermark-js-plus",
  "version": "1.6.3",
  "description": "watermark for the browser",
  "scripts": {
    "clean:dist": "rimraf dist",
    "prepare": "husky",
    "lint": "npx eslint \"src/**/*.{ts,js}\"",
    "lint-staged": "lint-staged",
    "dev": "concurrently \"npm run src:dev\" \"npm run docs:dev\"",
    "src:dev": "rollup -c --bundleConfigAsCjs --watch",
    "test": "jest --no-cache",
    "build:types": "tsc",
    "build": "npm run clean:dist && npm run build:types && rollup -c --bundleConfigAsCjs --environment NODE_ENV:production",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 -n changelog-option.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint-changelog": "concurrently \"npm run lint\" \"npm run changelog\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhensherlock/watermark-js-plus.git"
  },
  "author": "zhensherlock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhensherlock/watermark-js-plus/issues"
  },
  "homepage": "https://zhensherlock.github.io/watermark-js-plus/",
  "keywords": [
    "watermark",
    "canvas",
    "水印",
    "暗水印",
    "盲水印",
    "数据水印",
    "防删除水印",
    "解密水印"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "webpack": "dist/index.esm.js",
  "browser": "dist/index.esm.js",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": {
        "node": "./index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.cjs.js"
    },
    "./es": {
      "types": "./dist/types/index.d.ts",
      "import": {
        "default": "./dist/es/src/index.js"
      }
    },
    "./ie": {
      "types": "./dist/types/index.d.ts",
      "import": {
        "node": "./index.mjs",
        "default": "./dist/ie/index.esm.js"
      },
      "require": "./dist/ie/index.cjs.js"
    },
    "./ie/es": {
      "types": "./dist/types/index.d.ts",
      "import": {
        "default": "./dist/ie/es/src/index.ie.js"
      }
    },
    "./style.css": "./dist/es/style.css",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": [
    "./src/style/**/*",
    "**/style.css",
    "**/ie/**/*"
  ],
  "files": [
    "dist",
    "index.js",
    "index.mjs",
    "index.cjs",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@jest/globals": "^30.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jquery": "^3.5.32",
    "@types/markdown-it": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vue/theme": "^2.3.0",
    "autoprefixer": "^10.4.21",
    "canvas": "^2.11.2",
    "codecov": "^3.8.3",
    "concurrently": "^8.2.2",
    "conventional-changelog-angular": "^7.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "core-js": "^3.42.0",
    "cross-fetch": "^4.1.0",
    "cssnano": "^7.0.7",
    "element-plus": "^2.9.10",
    "eslint": "^9.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jimp": "^0.22.12",
    "jquery": "^3.7.1",
    "lint-staged": "^16.0.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "pinia": "^3.0.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.15.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "rollup-plugin-your-function": "^0.5.3",
    "terser": "^5.39.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "unplugin-element-plus": "^0.10.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13",
    "whatwg-fetch": "^3.6.20"
  },
  "overrides": {
    "@rollup/plugin-eslint": {
      "eslint": "$eslint"
    }
  }
}
